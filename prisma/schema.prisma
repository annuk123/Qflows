generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_UNPOOLED")
}

model Testimonial {
  userName  String
  avatarUrl String?
  rating    Int
  review    String
  createdAt DateTime @default(now())
  place     String?
  id        Int      @id @default(autoincrement())
}

generator client {
  provider = "prisma-client-js"
  output   = "./node_modules/@prisma/client"
}


datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Testimonial {
  userName  String
  avatarUrl String?
  rating    Int
  review    String
  createdAt DateTime @default(now())
  place     String?
  id        String   @id @default(uuid())
  userId    String
}
